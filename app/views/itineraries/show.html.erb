<div class="container">
  <div class="parent1">
    <div class="container event-container">
      <div class="col">
        <h1 class="h2 m-4 d-flex justify-content-center content5">Here is our recommended itinerary. Feel free to edit!</h1>
      </div>
      <div class="flex-container justify-content-center" id="event" >
        <% @events.each do |event| %>
          <%= render "event_infos", event: event, itinerary: @itinerary %>
        <% end %>
      </div>
    </div>
  </div>
  <%= button_to 'Confirm Itinerary', itineraries_path(@itinerary), method: :get, class: 'btn btn-pink border2' %>
</div>
