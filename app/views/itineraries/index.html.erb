<%# <%# <!-- %>
<div class="container">
  <div class="parent1 parent-height">
    <div class="container">
      <div class="col">
        <h1 class="h2 m-3 d-flex justify-content-center">Events</h1>
      </div>
      <div class="flex-container" id="event" >
        <% @events.each do |event| %>
          <%= render "event_infos", event: event %>
        <% end %>
      </div>
  </div>
</div>



<div>
  <% @itineraries.each do |itinerary| %>
    <p><%= itinerary %></p>
    <% itinerary.events.each do |event| %>
      <p><%= event %></p>
      <p><%= event.place %></p>
    <% end %>
  <% end %>
</div>
